post/index
<% @posts.each do |post| %>
<div>
  <div>
    <div>
      <%= link_to post_path(post.id) do %>
      <%= attachment_image_tag post, :image %>
      <% end %>
      <p><%#= image_tag('sample-author1.jpg') %></p>
      <p><%= post.title %></p>
      <p><%=post.user.name %></p>
    </div>
    <% end %>
  </div>
</div>